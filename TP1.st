Error subclass: #AbonoMensualInvalido	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #DuracionInvalida	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #HorarioInvalido	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Llamada	instanceVariableNames: 'duracion horario costo tarifa pais'	classVariableNames: ''	package: 'TP1'!!Llamada methodsFor: 'accessing' stamp: 'AlejoFabregas 4/25/2022 00:29'!establecerHorario: unHorario	horario := unHorario.! !!Llamada methodsFor: 'accessing' stamp: 'AlejoFabregas 4/25/2022 00:29'!establecerDuracion: unaDuracion	duracion := unaDuracion.! !!Llamada methodsFor: 'initialization' stamp: 'AlejoFabregas 4/25/2022 16:15'!initialize	duracion := 0.	horario := 0.	costo := 0.	tarifa := 0.	pais := 'Argentina'.! !!Llamada methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 16:38'!establecerTarifa: unaTarifa	tarifa := unaTarifa! !!Llamada methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 16:37'!calcularCosto	| costoCalculado |	costoCalculado := duracion * tarifa.	costoCalculado := self aplicarDescuento: costoCalculado.	costo := costoCalculado.	^ costoCalculado.! !!Llamada methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 04:10'!obtenerPais	^ pais.! !!Llamada methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 17:05'!aplicarDescuento: unCosto	(duracion >= 30) ifTrue: [ ^ unCosto - (unCosto * 0.1) ] ifFalse: [ ^ unCosto ]! !!Llamada methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 04:01'!obtenerDuracion	^ duracion.! !!Llamada methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 04:01'!obtenerHorario	^ horario.! !Llamada subclass: #LlamadaInternacional	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!LlamadaInternacional methodsFor: 'accessing' stamp: 'AlejoFabregas 4/25/2022 04:11'!establecerPais: unPais	pais := unPais.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LlamadaInternacional class	instanceVariableNames: ''!!LlamadaInternacional class methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 01:49'!llamarConDuracion: unaDuracion EnElHorario: unHorario Hacia: unPais	| unaLlamadaInternacional |	unaLlamadaInternacional := self new.	unaLlamadaInternacional establecerDuracion: unaDuracion.	unaLlamadaInternacional establecerHorario: unHorario.	unaLlamadaInternacional establecerPais: unPais.	^ unaLlamadaInternacional.! !Llamada subclass: #LlamadaNacional	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!LlamadaNacional class	instanceVariableNames: ''!!LlamadaNacional class methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 01:48'!llamarConDuracion: unaDuracion EnElHorario: unHorario	| unaLlamadaNacional unaTarifa |	unaLlamadaNacional :=  self new.	unaLlamadaNacional establecerDuracion: unaDuracion.	unaLlamadaNacional establecerHorario: unHorario.	unaTarifa := 0.1.	(unHorario between: 8 and: 19)	ifTrue: [ unaTarifa := 0.2 ].	unaLlamadaNacional establecerTarifa: unaTarifa.	^ unaLlamadaNacional.! !Error subclass: #NoHayLlamadasRegistradas	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #RegistroLlamadas	instanceVariableNames: 'registroLlamadas'	classVariableNames: ''	package: 'TP1'!!RegistroLlamadas methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 03:49'!agregarLlamada: unaLlamada	registroLlamadas add: unaLlamada.! !!RegistroLlamadas methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 01:58'!obtenerLlamadaMasCostosa	| llamadaMasCostosa |	llamadaMasCostosa := registroLlamadas detectMax: [ :llamada | llamada calcularCosto ].	[ llamadaMasCostosa ifNil: [ NoHayLlamadasRegistradas signal ].	] on: NoHayLlamadasRegistradas do: [ ^ 'No hay ninguna llamada en el registro de llamadas' ].	^ 'La llamada mas costosa: La llamada se hizo a ', (llamadaMasCostosa obtenerPais) asString, ' a las ', (llamadaMasCostosa obtenerHorario) asString, ' hs durando ', (llamadaMasCostosa obtenerDuracion) asString, ' minutos'.! !!RegistroLlamadas methodsFor: 'initialization' stamp: 'AlejoFabregas 4/25/2022 03:32'!initialize 	registroLlamadas := OrderedCollection new.! !Error subclass: #TarifaInvalida	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #TeleAlgo	instanceVariableNames: 'abonoMensual costoLlamadas tarifaInternacional registroLlamadas'	classVariableNames: ''	package: 'TP1'!!TeleAlgo methodsFor: 'accessing' stamp: 'AlejoFabregas 4/24/2022 19:21'!abonoMensual: unAbonoMensual	abonoMensual := unAbonoMensual.! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 02:17'!registrarLlamadaNacionalConDuracion: unaDuracion HechaEnElHorario: unHorario	| unaLlamadaNacional |	(unaDuracion < 0) ifTrue: [ DuracionInvalida signal: 'La duracion debe ser mayor o igual a 0' ].	(unHorario between: 1 and: 25) ifFalse: [ HorarioInvalido signal: 'El horario debe ser un numero natural que va desde la hora 1 a la hora 24' ].	unaLlamadaNacional := LlamadaNacional llamarConDuracion: unaDuracion EnElHorario: unHorario.	costoLlamadas := costoLlamadas + (unaLlamadaNacional calcularCosto).	registroLlamadas agregarLlamada: unaLlamadaNacional.! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 16:46'!consultarPorLlamadaEfectuadaMasCostosa	^ registroLlamadas obtenerLlamadaMasCostosa.! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/25/2022 16:47'!initialize  	abonoMensual := 0.	costoLlamadas := 0.	tarifaInternacional := 0.	registroLlamadas := RegistroLlamadas new.! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 01:51'!aplicarTarifaPorMinutoALlamadaInternacional: unaTarifa	(unaTarifa < 0) ifTrue: [ TarifaInvalida signal: 'La tarifa debe ser mayor o igual a 0' ].	tarifaInternacional := unaTarifa.! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/24/2022 23:31'!calcularGastoFinal	^ abonoMensual + costoLlamadas.! !!TeleAlgo methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 01:39'!registrarLlamadaInternacionalCon: unaDuracion HechaEnElHorario: unHorario Hacia: unPais	| unaLlamadaInternacional |	(unaDuracion < 0) ifTrue: [ DuracionInvalida signal: 'La duracion debe ser mayor o igual a 0' ].	(unHorario between: 1 and: 25) ifFalse: [ HorarioInvalido signal: 'El horario debe ser un numero natural que va desde la hora 1 a la hora 24' ].	unaLlamadaInternacional := LlamadaInternacional llamarConDuracion: unaDuracion EnElHorario: unHorario Hacia: unPais.	unaLlamadaInternacional establecerTarifa: tarifaInternacional.	costoLlamadas := costoLlamadas + (unaLlamadaInternacional calcularCosto).	registroLlamadas agregarLlamada: unaLlamadaInternacional.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!TeleAlgo class	instanceVariableNames: ''!!TeleAlgo class methodsFor: 'as yet unclassified' stamp: 'AlejoFabregas 4/26/2022 01:52'!conAbonoMensual: unAbonoMensual	| unTeleAlgo |	(unAbonoMensual < 0 ) ifTrue: [ AbonoMensualInvalido signal: 'El abono mensual debe ser mayor o igual a 0' ].	unTeleAlgo := self new.	unTeleAlgo abonoMensual: unAbonoMensual.	^ unTeleAlgo.! !